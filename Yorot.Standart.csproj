<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Yorot</RootNamespace>
    <Version>1.0.0.0</Version>
    <Authors>Haltroy</Authors>
    <Description>Standart library for all Yorot flavors.</Description>
    <PackageProjectUrl>https://github.com/Haltroy/Yorot</PackageProjectUrl>
    <OutputType>Library</OutputType>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="src\YorotVirtualDrive.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="src\YorotVirtualDrive.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Foster" Version="2.0.0.1" />
    <PackageReference Include="Fostrian" Version="1.1.0" />
    <PackageReference Include="HTAlt.Standart" Version="0.1.7.7" />
  </ItemGroup>

</Project>
